# OpenAI API Key for default model
# OPENAI_USER_KEY=your_openai_api_key_here

# Custom Model Factory path (optional)
# Uncomment and set this to use your own model factory implementation
# CUSTOM_MODEL_FACTORY=/path/to/your/custom_factory.py

# Example API Keys for other model providers
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# NVIDIA_API_KEY=your_nvidia_api_key_here

# Benchmark settings (defaults shown in comments)
# BENCHMARK_THREADS=1
# BENCHMARK_PREFIX=work
# DEFAULT_MODEL=gpt-4o-mini

# Docker timeouts in seconds (defaults shown in comments)
# DOCKER_TIMEOUT=600
# DOCKER_TIMEOUT_AGENT=600
# MODEL_TIMEOUT=60
# TASK_TIMEOUT=300
# QUEUE_TIMEOUT=disabled

# Docker directory size monitoring (defaults shown in comments)
# DOCKER_QUOTA_THRESHOLD_MB=50
# DOCKER_QUOTA_CHECK_INTERVAL=1
# DOCKER_QUOTA_MIN_COMPRESS_SIZE_MB=10

# EDA Tool Infrastructure Configuration (optional)
# VERIF_EDA_IMAGE=cvdp-cadence-verif:latest
# LICENSE_NETWORK=licnetwork
# LICENSE_NETWORK_AUTO_CREATE=true
# OSS_SIM_IMAGE=ghcr.io/hdl/sim/osvb
# OSS_PNR_IMAGE=ghcr.io/hdl/impl/pnr

# Enable LLM-based subjective scoring (defaults shown in comments)
# ENABLE_SUBJECTIVE_SCORING=false 
